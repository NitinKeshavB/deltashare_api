# Databricks Configuration
DLTSHR_WORKSPACE_URL=https://adb-xxxxx.azuredatabricks.net/

# Databricks OAuth Authentication
CLIENT_ID=your-client-id
CLIENT_SECRET=your-client-secret
ACCOUNT_ID=your-account-id

# Azure Blob Storage Logging (Optional)
ENABLE_BLOB_LOGGING=false
AZURE_STORAGE_ACCOUNT_URL=https://yourstorageaccount.blob.core.windows.net
AZURE_STORAGE_LOGS_CONTAINER=deltashare-logs

# PostgreSQL Logging (Optional)
ENABLE_POSTGRESQL_LOGGING=false
POSTGRESQL_CONNECTION_STRING=postgresql://user:password@your-postgres-server.postgres.database.azure.com:5432/deltashare_db?sslmode=require
POSTGRESQL_LOG_TABLE=application_logs
POSTGRESQL_MIN_LOG_LEVEL=WARNING
